<!DOCTYPE html>
<html>

<head>
    {{>head}}
    <title>{{title}}</title>
</head>

<body>
    <div class="main-content">
        {{>pageheader}}
        <div class="form-group">
            <label for="address">Address To Locate:</label>
            <input class="form-control input-lg" type="text" id="address"
                placeholder="Type Address Here and Hit <ENTER>" onkeyup="doLocation(event,this)" autofocus />
        </div>
        <div class="row">
            <div class="col-md-6">
                <div class="panel panel-primary">
                    <div class="panel-heading"> <div id="locurl1">Location 1</div> </div>
                    <div class="panel-body">
                        <div id="location1">Details Will Be Here..</div>
                        <p />
                        <div id="maploc1">Map Will Be Here...</div>
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="panel panel-primary">
                    <div class="panel-heading"> <div id="locurl2">Location 2</div> </div>
                    <div class="panel-body">
                        <div id="location2">Details Will Be Here...</div>
                        <p />
                        <div id="maploc2">Map Will Be Here...</div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    {{>pagefooter}}
</body>
